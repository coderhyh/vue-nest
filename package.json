{
  "name": "vue+nest",
  "type": "module",
  "version": "1.0.0",
  "description": "vue+nest template",
  "author": "coderhyh",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=16.17.1"
  },
  "scripts": {
    "start": "pnpm -r start",
    "start:web": "pnpm --filter web start",
    "start:service": "pnpm --filter service start",
    "init": "pnpm i && git init",
    "prepare": "husky install",
    "commit": "git add . && cz",
    "lint": "npm run lint:fix && npm run lint:style",
    "lint:fix": "eslint . --fix",
    "lint:style": "pnpm --filter web exec stylelint \"./src/**/*.{css,less,vue,html}\" --fix",
    "g": "node ./scripts/nestGenerate.js"
  },
  "config": {
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@unocss/eslint-plugin": "^0.60.4",
    "commitizen": "4.3.0",
    "cz-customizable": "7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "husky": "^8.0.3",
    "unocss": "^0.58.9"
  }
}
